class lastoccurance 
{
	public static void lstdigoccur(String str,char ch)
	{
		int n=str.length();
		for(int i=n-1;i>=0;i--)
		{
			char ch1=str.charAt(i);
			if(ch1==ch)
			{
			 System.out.println(i);
			 break;
			}
		}
		
		
	}
	public static void main(String[] args) 
	{
        lstdigoccur("Manasareddy",'a');
	}
}
