class palindrome 
{
	public static void main(String[] args) 
	{
		String str="malayalam";
		String res="";
		int n=str.length;
		for(int i=n-1;i>=0;i--){

		char ch=str.charAt(i);
		res+=ch;
		}
		if (str==res)
		{
			System.out.println("palindrome");
		}
		else{
			
         System.out.println("not palindrome");

	   }
}
}
